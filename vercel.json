{
  "version": 2,
  "builds": [
    {
      "src": "web/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/vat/validate",
      "dest": "web/server.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/health",
      "dest": "web/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "web/server.js"
    }
  ]
}
